<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="fill" android:id="@id/actionbar_background">
        <shape android:tint="@color/op_control_bg_color_default" android:shape="rectangle">
            <solid android:color="@android:color/white" />
        </shape>
    </item>
    <item android:gravity="fill_horizontal|bottom|center" android:id="@id/actionbar_divider">
        <shape android:tint="@color/op_control_divider_color_default" android:shape="rectangle">
            <size android:height="2.0px" />
            <solid android:color="@android:color/white" />
        </shape>
    </item>
</layer-list>