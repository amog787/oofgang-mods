<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen android:persistent="false" android:title="@string/accessibility_settings" android:key="accessibility_settings_screen"
  xmlns:android="http://schemas.android.com/apk/res/android" xmlns:settings="http://schemas.android.com/apk/res-auto">
    <PreferenceCategory android:persistent="false" android:title="@string/user_installed_services_category_title" android:key="user_installed_services_category" />
    <PreferenceCategory android:persistent="false" android:title="@string/screen_reader_category_title" android:key="screen_reader_category" settings:searchable="false">
        <Preference android:persistent="false" android:title="@string/tts_settings_title" android:key="tts_settings_preference" android:fragment="com.android.settings.tts.TextToSpeechSettings" settings:controller="com.android.settings.language.TtsPreferenceController" />
    </PreferenceCategory>
    <PreferenceCategory android:persistent="false" android:title="@string/display_category_title" android:key="display_category">
        <Preference android:persistent="false" android:title="@string/title_font_size" android:key="font_size_preference_screen" android:fragment="com.android.settings.display.ToggleFontSizePreferenceFragment" settings:controller="com.android.settings.display.FontSizePreferenceController" />
        <com.android.settings.display.ScreenZoomPreference android:persistent="false" android:title="@string/screen_zoom_title" android:key="accessibility_settings_screen_zoom" android:fragment="com.android.settings.display.ScreenZoomSettings" />
        <Preference android:icon="@drawable/ic_accessibility_magnification" android:persistent="false" android:title="@string/accessibility_screen_magnification_title" android:key="magnification_preference_screen" android:fragment="com.android.settings.accessibility.MagnificationPreferenceFragment" settings:controller="com.android.settings.accessibility.MagnificationPreferenceController" />
        <SwitchPreference android:persistent="false" android:title="@string/accessibility_toggle_large_pointer_icon_title" android:key="toggle_large_pointer_icon" settings:controller="com.android.settings.accessibility.LargePointerIconPreferenceController" />
        <SwitchPreference android:persistent="false" android:title="@string/accessibility_disable_animations" android:key="toggle_disable_animations" settings:controller="com.android.settings.accessibility.DisableAnimationsPreferenceController" />
    </PreferenceCategory>
    <PreferenceCategory android:persistent="false" android:title="@string/interaction_control_category_title" android:key="interaction_control_category">
        <Preference android:persistent="false" android:title="@string/accessibility_autoclick_preference_title" android:key="autoclick_preference" android:fragment="com.android.settings.accessibility.ToggleAutoclickPreferenceFragment" android:iconSpaceReserved="false" settings:controller="com.android.settings.accessibility.AutoclickPreferenceController" />
        <SwitchPreference android:persistent="false" android:title="@string/accessibility_power_button_ends_call_prerefence_title" android:key="toggle_power_button_ends_call_preference" settings:controller="com.android.settings.accessibility.PowerButtonEndsCallPreferenceController" />
        <SwitchPreference android:persistent="false" android:title="@string/accelerometer_title" android:key="toggle_lock_screen_rotation_preference_accessibility" settings:controller="com.android.settings.accessibility.LockScreenRotationPreferenceController" />
        <ListPreference android:persistent="false" android:entries="@array/long_press_timeout_selector_titles" android:title="@string/accessibility_long_press_timeout_preference_title" android:key="select_long_press_timeout_preference" android:entryValues="@array/long_press_timeout_selector_values" settings:controller="com.android.settings.accessibility.SelectLongPressTimeoutPreferenceController" />
        <ListPreference android:persistent="false" android:entries="@array/accessibility_timeout_summaries" android:title="@string/accessibility_setting_item_control_timeout_title" android:key="accessibility_control_timeout_preference" android:entryValues="@array/accessibility_timeout_selector_values" settings:controller="com.android.settings.accessibility.AccessibilityTimeoutPreferenceController" />
        <Preference android:persistent="false" android:title="@string/accessibility_vibration_settings_title" android:key="vibration_preference_screen" android:fragment="com.android.settings.accessibility.VibrationSettings" settings:controller="com.android.settings.accessibility.VibrationPreferenceController" />
        <Preference android:persistent="false" android:title="@string/system_navigation_title" android:key="gesture_system_navigation_input_summary_accessibility" android:fragment="com.android.settings.gestures.SystemNavigationGestureSettings" settings:controller="com.android.settings.gestures.SystemNavigationPreferenceController" settings:searchable="false" />
    </PreferenceCategory>
    <PreferenceCategory android:persistent="false" android:title="@string/audio_and_captions_category_title" android:key="audio_and_captions_category">
        <Preference android:persistent="false" android:title="@string/live_caption_title" android:key="live_caption" android:summary="@string/live_caption_summary" android:iconSpaceReserved="false" settings:controller="com.android.settings.accessibility.LiveCaptionPreferenceController" settings:searchable="false" />
        <SwitchPreference android:persistent="false" android:title="@string/accessibility_toggle_master_mono_title" android:key="toggle_master_mono" android:summary="@string/accessibility_toggle_master_mono_summary" settings:controller="com.android.settings.accessibility.MasterMonoPreferenceController" />
        <com.android.settings.accessibility.BalanceSeekBarPreference android:persistent="false" android:title="@string/accessibility_toggle_master_balance_title" android:key="seekbar_master_balance" android:iconSpaceReserved="false" />
        <Preference android:persistent="false" android:title="@string/accessibility_hearingaid_title" android:key="hearing_aid_preference" android:summary="@string/accessibility_hearingaid_not_connected_summary" android:iconSpaceReserved="false" settings:controller="com.android.settings.accessibility.AccessibilityHearingAidPreferenceController" />
        <Preference android:title="@string/rtt_settings_title" android:key="rtt_setting" android:summary="@string/summary_placeholder" settings:controller="com.android.settings.accessibility.RTTSettingPreferenceController" />
        <SwitchPreference android:persistent="false" android:title="@string/oneplus_hearing_aid" android:key="hearing_aid" settings:controller="com.android.settings.accessibility.OPHearingAidPreferenceController" />
        <Preference android:persistent="false" android:title="@string/oneplus_rtt_feature" android:key="rtt_feature" android:summary="@string/switch_off_text" android:iconSpaceReserved="false" settings:controller="com.android.settings.accessibility.OPRTTFeatureSettingPreferenceController" />
        <Preference android:persistent="false" android:title="@string/accessibility_captioning_title" android:key="captioning_preference_screen" android:fragment="com.android.settings.accessibility.CaptionPropertiesFragment" settings:controller="com.android.settings.accessibility.CaptioningPreferenceController" />
    </PreferenceCategory>
    <PreferenceCategory android:persistent="false" android:title="@string/experimental_category_title" android:key="experimental_category" settings:initialExpandedChildrenCount="1">
        <SwitchPreference android:persistent="false" android:title="@string/accessibility_toggle_high_text_contrast_preference_title" android:key="toggle_high_text_contrast_preference" settings:controller="com.android.settings.accessibility.HighTextContrastPreferenceController" />
        <Preference android:icon="@drawable/ic_daltonizer" android:persistent="false" android:title="@string/accessibility_display_daltonizer_preference_title" android:key="daltonizer_preference" android:fragment="com.android.settings.accessibility.ToggleDaltonizerPreferenceFragment" settings:controller="com.android.settings.accessibility.DaltonizerPreferenceController" />
        <Preference android:icon="@drawable/ic_color_inversion" android:persistent="false" android:title="@string/accessibility_display_inversion_preference_title" android:key="toggle_inversion_preference" android:fragment="com.android.settings.accessibility.ToggleColorInversionPreferenceFragment" settings:controller="com.android.settings.accessibility.ColorInversionPreferenceController" />
        <SwitchPreference android:persistent="false" android:title="@string/accessibility_shortcut_service_on_lock_screen_title" android:key="accessibility_shortcut_preference" android:summary="@string/accessibility_shortcut_description" settings:controller="com.android.settings.accessibility.AccessibilityShortcutPreferenceController" />
    </PreferenceCategory>
</PreferenceScreen>